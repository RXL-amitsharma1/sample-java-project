<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Checkstyle//DTD Checkstyle Configuration 1.3//EN" "https://checkstyle.org/dtds/configuration_1_3.dtd">
<?xml version="1.0" ?>
<module xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Checker" xsi:noNamespaceSchemaLocation="https://checkstyle.org/dtds/configuration_1_3.dtd">
  <module name="BeforeExecutionExclusionFileFilter"/>
  <module name="FileTabCharacter"/>
  <module name="NewlineAtEndOfFile"/>
  <module name="TreeWalker">
    <module name="Checkstyle_AbbreviationAsWordInName">
      <property name="severity" value="warning"/>
      <property name="ignoreStaticFinal" value="true"/>
      <property name="ignoreOverriddenMethods" value="true"/>
      <property name="allowedAbbreviations" value=""/>
      <property name="allowedAbbreviationLength" value="3"/>
      <property name="ignoreStatic" value="true"/>
      <property name="tokens" value="CLASS_DEF, INTERFACE_DEF, ENUM_DEF, ANNOTATION_DEF, ANNOTATION_FIELD_DEF, PARAMETER_DEF, VARIABLE_DEF, METHOD_DEF, PATTERN_VARIABLE_DEF, RECORD_DEF, RECORD_COMPONENT_DEF"/>
      <property name="ignoreFinal" value="true"/>
    </module>
    <module name="Checkstyle_ArrayTypeStyle">
      <property name="severity" value="warning"/>
      <property name="javaStyle" value="true"/>
    </module>
    <module name="Checkstyle_AvoidNestedBlocks">
      <property name="severity" value="warning"/>
      <property name="allowInSwitchCase" value="false"/>
    </module>
    <module name="Checkstyle_AvoidStarImport">
      <property name="severity" value="warning"/>
      <property name="allowClassImports" value="false"/>
      <property name="allowStaticMemberImports" value="false"/>
      <property name="excludes" value=""/>
    </module>
    <module name="Checkstyle_AvoidStaticImport">
      <property name="severity" value="warning"/>
      <property name="excludes" value=""/>
    </module>
  </module>
</module>
